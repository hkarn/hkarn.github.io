{
  "name": "portfolio-and-cv-site",
  "homepage": "https://hkarn.github.io/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "arrow-keys-react": "^1.0.6",
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.6",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-google-invisible-recaptcha": "^0.2.6",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-localize-redux": "^2.16.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-textfit": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "swipe-react": "^1.0.4",
    "wheel-react": "^1.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --output-style expanded --source-map true",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --watch --recursive --output-style expanded --source-map true",
    "build-css-prod": "node-sass-chokidar ./src/styles/scss/ -o ./src/styles/css/ --output-style compressed -x",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css-prod build-js",
    "test": "react-scripts test --env=jsdom",
    "deploy": "run-s build deploy-ghpages",
    "deploy-ghpages": "gh-pages -b master -d build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2"
  }
}
